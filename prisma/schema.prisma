generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  firstname String
  lastname String
  username String @unique
  password String
  profilepicture String?
}

model PC {
  id String @id @default(uuid())
  macaddr String
  hostname String
  Seriennummer String @unique
  domain String
  
}
